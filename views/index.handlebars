 <h2>Burgers That Are not Eaten!</h2> 
</form>
<ul>
  {{#each burgers}}
    {{#unless eaten}}
      {{> burgers/burger-block hung=true}}
    {{/unless}}
  {{/each}}
</ul>


<h2>Burgers That Are Eaten!</h2>
<ul>
  {{#each burgers}}
    {{#if eaten}}
      {{> burgers/burger-block hung=true}}
    {{/if}}
  {{/each}}
</ul>

<h2>Eat A Burger</h2>
<form class="create-form">

  <div class="form-group">
    <label for="ca">Burger Name:</label>
    <input type="text" id="ca" name="name">
  </div>

  <div class="form-group">
    <label for="hung"></label>
    <input type="radio" name="eaten" value="0"><button class="btn btn-outline-danger" type="submit">Submit</button><br>
  </div>

